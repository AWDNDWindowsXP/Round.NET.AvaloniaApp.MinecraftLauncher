<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="RMCL.Views.Pages.DialogPage.User.AddUserDialog">
    <DockPanel LastChildFill="True">
        <TextBlock VerticalAlignment="Center" Margin="0,0,20,0">选择账户类型：</TextBlock>
        <ComboBox VerticalAlignment="Center" Width="200" SelectedIndex="1" Name="ComboBox" SelectionChanged="ComboBox_OnSelectionChanged">
            <ComboBoxItem Tag="off">离线账户</ComboBoxItem>
            <ComboBoxItem Tag="msa">正版账户</ComboBoxItem>
            <ComboBoxItem IsEnabled="False">第三方验证</ComboBoxItem>
        </ComboBox>
    </DockPanel>
</UserControl>
