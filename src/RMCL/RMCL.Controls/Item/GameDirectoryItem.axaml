<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:button="clr-namespace:RMCL.Controls.Button"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="80"
             x:Class="RMCL.Controls.Item.GameDirectoryItem">
    <StackPanel>
        <TextBlock Margin="5,5,5,0" FontSize="18" FontWeight="Bold" Name="DirName">Name</TextBlock>
        <TextBlock Margin="5,3,5,5" FontSize="12" TextTrimming="CharacterEllipsis" Name="DirPath">Path</TextBlock>
    </StackPanel>
</UserControl>
