<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:button="clr-namespace:RMCL.Controls.Button"
             mc:Ignorable="d" d:DesignWidth="800"
             x:Class="RMCL.Controls.Button.ProjectItemButton">
    <Border Background="{DynamicResource BackColor}" CornerRadius="6">
        <DockPanel>
            <button:IconButton Click="GoBtn_OnClick" Name="GoBtn" Icon="Link20Regular" Content="前往查看" DockPanel.Dock="Right" Margin="15"></button:IconButton>
            
            <Image Height="32" Name="ImageBox" Width="32" DockPanel.Dock="Left" Margin="15"></Image>
            <StackPanel DockPanel.Dock="Left" Margin="10" VerticalAlignment="Center">
                <TextBlock Name="TitleBox" FontSize="18">Title</TextBlock>
            </StackPanel>
        </DockPanel>
    </Border>
</UserControl>
