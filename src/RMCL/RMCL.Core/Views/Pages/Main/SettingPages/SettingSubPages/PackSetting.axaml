<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:button="clr-namespace:RMCL.Controls.Button;assembly=RMCL.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="RMCL.Core.Views.Pages.Main.SettingPages.SettingSubPages.PackSetting">
    <StackPanel Margin="5">
        <DockPanel Margin="5">
            <TextBlock VerticalAlignment="Center" DockPanel.Dock="Left">Round Studio 通用主题包</TextBlock>
            <button:AccentIconButton Name="AddPackBtn" Click="AddPackBtn_OnClick" DockPanel.Dock="Right" Margin="10,0,0,0" Content="添加" Icon="Folder20Regular" HorizontalAlignment="Right"/>
            <button:IconButton Name="UpdateBtn" Click="UpdateBtn_OnClick" DockPanel.Dock="Right" Margin="10,0,0,0" Content="刷新" Icon="ArrowClockwise20Regular" HorizontalAlignment="Right"/>
        </DockPanel>
        <ListBox ScrollViewer.VerticalScrollBarVisibility="Disabled" Name="ChoosePackListBox" SelectionChanged="ChoosePackListBox_OnSelectionChanged">
            
        </ListBox>
    </StackPanel>
</UserControl>
